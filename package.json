{
  "name": "vz-react-modal",
  "private": false,
  "version": "1.0.1",
  "author": "vzamboulingame",
  "description": "A simple react modal component created for OpenClassrooms Project #14",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vzamboulingame/vz-react-modal.git"
  },
  "keywords": [
    "react",
    "modal",
    "component",
    "typescript",
    "tailwindcss",
    "openclassrooms"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vz-react-modal.umd.cjs",
  "module": "./dist/vz-react-modal.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vz-react-modal.js",
      "require": "./dist/vz-react-modal.umd.cjs"
    }
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
